{
  "name": "cms-admin",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "use-dev": "cp -fr src/configs/config.dev.ts src/configs/config.ts",
    "use-prod": "cp -fr src/configs/config.prod.ts src/configs/config.ts",
    "use-local": "cp -fr src/configs/config.local.ts src/configs/config.ts",
    "start": "npm run use-dev && vite",
    "start-local": "npm run use-local && vite --host",
    "build": "npm run use-prod && tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write .",
    "dev": "npm run use-dev && tsc -b && NODE_ENV=development vite build --mode development"
  },
  "dependencies": {
    "@emotion/react": "^11.13.0",
    "@emotion/styled": "^11.13.0",
    "@mui/icons-material": "^5.16.7",
    "@mui/material": "^5.16.7",
    "@mui/x-date-pickers": "^7.11.0",
    "@tanstack/react-query": "^5.64.1",
    "@tiptap/extension-color": "^2.7.4",
    "@tiptap/extension-highlight": "^2.7.4",
    "@tiptap/extension-list-item": "^2.8.0",
    "@tiptap/extension-text-align": "^2.7.4",
    "@tiptap/extension-text-style": "^2.7.4",
    "@tiptap/extension-youtube": "^2.7.4",
    "@tiptap/pm": "^2.7.2",
    "@tiptap/react": "^2.7.2",
    "@tiptap/starter-kit": "^2.7.2",
    "date-fns": "2.30.0",
    "js-yaml": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.1",
    "react-toastify": "^10.0.6",
    "recharts": "^2.12.7",
    "rxjs": "^7.8.1",
    "vite-plugin-remove-console": "^2.2.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.11",
    "jest": "^29.7.0",
    "prettier": "3.3.3",
    "sass": "^1.77.8",
    "ts-jest": "^29.2.5",
    "typescript": "^5.8.2",
    "vite": "^5.4.6",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.2"
  }
}
